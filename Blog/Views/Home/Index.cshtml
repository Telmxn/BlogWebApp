@model IEnumerable<Post>

<h2>Home Page!</h2>

<img src="~/content/static/pexels-min-an-1448709.jpg" style="width:500px;" />
<div>
    <ul>
        @foreach(var post in Model)
        {
        <li>
            <a asp-controller="Home" asp-action="Post" asp-route-id="@post.Id">
                @post.Title
            </a>
        </li>
        }
    </ul>
</div>